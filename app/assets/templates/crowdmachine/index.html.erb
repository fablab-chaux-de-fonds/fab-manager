<section class="heading b-b">
  <div class="row no-gutter">
    <div class="col-xs-2 col-sm-2 col-md-1">
      <section class="heading-btn">
        <a href="#" ng-click="backPrevLocation($event)">
          <i class="fa fa-long-arrow-left "></i>
        </a>
      </section>
    </div>
    <div class="col-xs-10 col-sm-10 col-md-8 b-l b-r-md hide-b-r-lg">
      <section class="heading-title">
        <h1>{{ 'crowdmachine.crowdmachine' | translate}}</h1>
      </section>
    </div>
  </div>
</section>

<section class="row no-gutter">
  <!-- <div class="crowdmachine-card wrapper-lg"> -->
  <div class="wrapper-lg">
    <div ng-repeat="i in data" class="crowdmachine-repeat col-xs-12 col-sm-6 col-md-4 col-lg-3">
      <div class="card crowdmachine-card">
        <%= image_tag("/assets/crowdmachine/{{ i.id }}_800x800.png", class: 'img-crowdmachine') %>
        <div class="card-body">
          <h5 class="card-title">{{ i.title }}</h5>
          <p class="card-text">Some quick example text to build on the card title and make up the bulk of the card's content.</p>
          <a href="#" class="btn btn-primary">Go somewhere</a>
        </div>
      </div>
    </div>
  </div>
</section>
