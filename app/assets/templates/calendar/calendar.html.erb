<section class="heading b-b">
    <div class="row no-gutter">
      <div class="col-xs-4 col-sm-2 col-md-1">
        <section class="heading-btn">
          <a href="#" ng-click="backPrevLocation($event)"><i class="fa fa-long-arrow-left "></i></a>
        </section>
      </div>
      <div class="col-xs-8 col-sm-10 col-md-8 b-l b-r-md hide-b-r-lg">
        <section class="heading-title">
          <h1 class="m-l-xs" translate>{{ 'calendar.calendar' }}</h1>
          <div class="heading-actions hidden-md hidden-lg m-r">
            <button type="button" class="btn btn-default" ng-click="openFilterAside()">
              <span class="fa fa-filter"></span>
            </button>
          </div>
        </section>
      </div>
      <div class="col-xs-12 col-sm-12 col-md-3 b-t hide-b-md hidden-lg hidden-xs">
        <div class="heading-actions wrapper">
          <button type="button" class="btn btn-default m-t m-b" ng-click="openFilterAside()">
            <span class="fa fa-filter"></span> {{ 'calendar.filter_calendar' | translate }}
          </button>
        </div>
      </div>
    </div>
  </section>
<div class="calendar-container">
  <div class="calendar-expander">
      <div ui-calendar="calendarConfig" ng-model="eventSources" calendar="calendar" class="public-calendar"></div>  
  </div>
</div>

